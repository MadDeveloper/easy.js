{
  "name": "Easy.js",
  "version": "1.0.0",
  "description": "Easy.js framework",
  "main": "src/app.js",
  "repository": "https://jsergent-prepapp@bitbucket.org/prepapp/easy.js.git",
  "scripts": {
    "clean-build": "rm -rf build/*",
    "build": "node_modules/.bin/babel -w src/ -d build/",
    "build-maps": "node_modules/.bin/babel -w src/ -d build/ -s",
    "test": "npm run bundle-tests",
    "bundle-tests": "node build/bin/bundles/tests",
    "bundle-new": "node build/bin/bundles/new",
    "db-migrate": "node build/bin/database/migrate",
    "db-import": "node build/bin/database/import",
    "db-startup": "node build/bin/database/startup",
    "db-startup-lite": "node build/bin/database/startup --lite",
    "debug": "node --expose-gc debug build/app.js",
    "start": "node --expose-gc build/app.js",
    "start-prod": "node --expose-gc build/app.js --prod",
    "start-forever": "node --expose-gc build/app.js --prod > stdout.txt 2> stderr.txt &",
    "start-forever-dev": "node --expose-gc build/app.js > stdout.txt 2> stderr.txt &"
  },
  "keywords": [
    "node",
    "js",
    "api",
    "rest",
    "restful",
    "framework"
  ],
  "author": "Julien Sergent",
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.14.1",
    "bookshelf": "~0.9.1",
    "colors": "^1.1.2",
    "compression": "^1.6.0",
    "cors": "^2.7.1",
    "express": "^4.13.3",
    "helmet": "^1.0.1",
    "https": "~1.0.0",
    "jsonwebtoken": "^5.4.1",
    "knex": "~0.9.0",
    "ksort": "0.0.6",
    "lodash": "~4.0.0",
    "minimist": "^1.2.0",
    "morgan": "~1.6.1",
    "multiparty": "^4.1.2",
    "mysql": "~2.10.2",
    "numeral": "^1.5.3",
    "path": "^0.12.7",
    "pathinfo": "^0.1.0",
    "readline-sync": "^1.3.0",
    "uniqid": "^1.0.0",
    "when": "~3.7.5"
  },
  "devDependencies": {
    "babel-cli": "^6.8.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-preset-stage-2": "^6.5.0",
    "execsql": "0.0.3",
    "mocha": "~2.3.4",
    "unit.js": "^2.0.0",
    "walk": "^2.3.9"
  }
}
