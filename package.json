{
  "name": "Easy.js",
  "version": "1.0.0",
  "description": "Easy.js framework",
  "main": "app.js",
  "repository": "https://jsergent-prepapp@bitbucket.org/prepapp/easy.js.git",
  "scripts": {
    "test": "npm run bundle-tests",
    "bundle-tests": "node bin/bundles/tests",
    "bundle-new": "node bin/bundles/new",
    "db-migrate": "node bin/database/migrate",
    "db-import": "node bin/database/import",
    "db-startup": "node bin/database/startup",
    "db-startup-lite": "node bin/database/startup --lite",
    "start": "node --expose-gc app.js",
    "start-prod": "node --expose-gc app.js --prod",
    "start-forever": "node --expose-gc bin/application/forever"
  },
  "keywords": [
    "orm",
    "node",
    "api",
    "js",
    "rest",
    "restful",
    "framework",
    "bundle"
  ],
  "author": "Julien Sergent",
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.14.1",
    "bookshelf": "~0.9.1",
    "colors": "^1.1.2",
    "compression": "^1.6.0",
    "cors": "^2.7.1",
    "express": "^4.13.3",
    "forever-monitor": "^1.7.0",
    "helmet": "^1.0.1",
    "https": "~1.0.0",
    "jsonwebtoken": "^5.4.1",
    "knex": "~0.9.0",
    "ksort": "0.0.6",
    "lodash": "~4.0.0",
    "minimist": "^1.2.0",
    "morgan": "~1.6.1",
    "multiparty": "^4.1.2",
    "mysql": "~2.10.2",
    "numeral": "^1.5.3",
    "path": "^0.12.7",
    "pathinfo": "^0.1.0",
    "readline-sync": "^1.3.0",
    "uniqid": "^1.0.0",
    "when": "~3.7.5"
  },
  "devDependencies": {
    "execsql": "0.0.3",
    "unit.js": "^2.0.0",
    "walk": "^2.3.9",
    "mocha": "~2.3.4"
  }
}
