{
  "name": "Easy.js",
  "version": "0.1.0",
  "description": "Easy.js Restful API framework",
  "main": "src/app.js",
  "repository": "https://github.com/MadDeveloper/easy.js.git",
  "scripts": {
    "bundles:new": "node bin/bundles/new",
    "bundles:restore:skeleton": "node bin/bundles/restore",
    "bundles:routes": "node bin/bundles/routes",
    "clean:logs": "rm -rf logs/*.log",
    "db:import": "node bin/database/import",
    "db:migrate": "node bin/database/migrate",
    "debug": "node --expose-gc --harmony debug src/app.js",
    "generate:doc": "apidoc -i src/ -e skeleton/ -o doc/",
    "start": "nodemon -w src --expose-gc --harmony src/app.js",
    "start:forever": "node --expose-gc --harmony src/app.js > stdout.txt 2> stderr.txt &",
    "test": "node bin/tests"
  },
  "keywords": [
    "node",
    "js",
    "api",
    "rest",
    "restful",
    "framework",
    "mvc",
    "javascript",
    "easy",
    "easyjs",
    "server",
    "es6",
    "es2015",
    "backend"
  ],
  "author": "Julien Sergent",
  "license": "MIT",
  "dependencies": {
    "app-module-path": "^2.1.0",
    "body-parser": "^1.15.2",
    "bookshelf": "^0.10.2",
    "colors": "^1.1.2",
    "compression": "^1.6.2",
    "cors": "^2.8.1",
    "express": "^4.14.0",
    "express-session": "^1.14.1",
    "helmet": "^2.3.0",
    "https": "~1.0.0",
    "jsonwebtoken": "^7.1.9",
    "knex": "~0.12.2",
    "lodash": "~4.16.3",
    "minimist": "^1.2.0",
    "mongoose": "^4.6.3",
    "morgan": "~1.7.0",
    "mysql": "^2.12.0",
    "numeral": "^1.5.3",
    "pad-right": "^0.2.2",
    "passport": "^0.3.2",
    "passport-local": "^1.0.0",
    "readline-sync": "^1.4.4",
    "when": "~3.7.7"
  },
  "devDependencies": {
    "copy-dir": "^0.3.0",
    "execsql": "0.0.3",
    "jasmine": "^2.5.2",
    "walk": "^2.3.9"
  }
}
